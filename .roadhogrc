{
  "entry": "src/index.js",
  "publicPath": "./",
  "autoprefixer": {
    "browsers": [
      "> 1%",
      "last 2 versions"
    ]
  },
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime",
        ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }]
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime"
      ]
    }
  }
}
